        "msedgedriver": "^91.0.0",
        "node-worker-threads-pool": "^1.5.0",
        "path": "^0.12.7",
        "popper.js": "^1.14.7",
        "react-scripts": "^3.4.1",
        "request-promise": "^4.2.6",
        "selenium-standalone": "^6.16.0",
        "selenium-webdriver": "^4.0.0-rc-1",
        "sgmf-scripts": "^2.3.0",
        "threads": "^1.6.5",
        "wdio-sauce-service": "^0.4.14",
        "webdriverio": "^5.14.5"
    },
    "scripts": {
        "cover": "sgmf-scripts --cover test/unit/**/*.js",
        "test": "sgmf-scripts --test test/unit/**/*.js",
        "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
        "test:acceptance:custom": "npx codeceptjs run --plugins retryFailedStep",
        "test:acceptance:deep": "npx codeceptjs run --plugins retryFailedStep --grep '(?=.*)^(?!.*@mobile)^(?!.*@tablet)^(?!.*@pageDesigner)'",
        "test:acceptance:smoke": "npx codeceptjs run --plugins retryFailedStep --grep @happyPath",
        "test:acceptance:pagedesigner": "npx codeceptjs run --plugins retryFailedStep --grep @pageDesigner --profile",
        "test:acceptance:desktop": "npx codeceptjs run --plugins retryFailedStep --grep '(?=.*)^(?!.*@mobile)^(?!.*@tablet)^(?!.*@pageDesigner)^(?!.*@deepTest)'",
        "test:acceptance:mobile": "npx codeceptjs run --plugins retryFailedStep --profile sauce:phone --grep @mobile",
        "test:acceptance:tablet": "npx codeceptjs run --plugins retryFailedStep --profile sauce:tablet --grep @tablet",
        "test:acceptance:parallel": "npx codeceptjs run-multiple parallel",
        "test:acceptance:multibrowsers": "npx codeceptjs run-multiple multibrowsers",
        "test:acceptance:report": "./node_modules/.bin/allure serve test/acceptance/report",
        "lint": "sgmf-scripts --lint js",
        "upload": "sgmf-scripts --upload",
        "uploadCartridge": "sgmf-scripts --uploadCartridge int_extend_sfra && sgmf-scripts --uploadCartridge int_extend",
        "watch": "sgmf-scripts --watch",
        "compile:js": "sgmf-scripts --compile js",
        "compile:scss": "sgmf-scripts --compile css",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples"
    },
    "paths": {
        "base": "../salesforce-commerce-cloud/cartridges/app_storefront_base/"
    }
}
